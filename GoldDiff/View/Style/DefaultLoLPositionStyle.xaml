<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controlElement="clr-namespace:GoldDiff.View.ControlElement"
                    xmlns:view="clr-namespace:GoldDiff.Shared.View;assembly=GoldDiff.Shared">

    <Style TargetType="{x:Type controlElement:LoLPositionView}">
        <Setter Property="Position" Value="Top" />
        <Setter Property="ActivePositionForeground" Value="{DynamicResource {x:Static view:GoldDiffSharedResourceKeys.ForegroundRegular}}" />
        <Setter Property="InactivePositionForeground" Value="{DynamicResource {x:Static view:GoldDiffSharedResourceKeys.ForegroundDisabled}}" />
    </Style>

</ResourceDictionary>