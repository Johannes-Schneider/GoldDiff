                <ResourceDictionary Source="pack://application:,,,/GoldDiff.Shared;component/View/Style/DefaultComboBoxStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/GoldDiff.Shared;component/View/Style/DefaultLabelStyle.xaml" />
                
